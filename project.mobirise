        {
          "_name": "accordion1",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".tabContent": {
              "display": "none",
              "order": "1",
              "width": "100%",
              "padding": "0 50px 0 15px",
              ".mbr-text": {
                "padding-left": "30px"
              }
            },
            ".accordion-container": {
              "padding": "0",
              "width": "100%"
            },
            ".tabButton": {
              "padding": "0 50px 0 15px",
              "&[selected]": {
                "outline": "none"
              },
              "&[selected] + .tabContent": {
                "display": "block"
              },
              "& when (@showIcons)": {
                "align-items": "center",
                ".card-title": {
                  "flex": "1 1"
                },
                ".icon": {
                  "position": "relative",
                  "display": "inline-block",
                  "flex": "0 0 auto",
                  "width": "~\"@{iconsSize}rem\"",
                  "height": "~\"@{iconsSize}rem\"",
                  "margin": "0 8px 0 0",
                  "&:before, &:after": {
                    "content": "\"\"",
                    "position": "absolute",
                    "left": "0",
                    "top": "0",
                    "background-color": "@primaryColor"
                  },
                  "&:before": {
                    "left": "~\"calc(50% - 1px)\"",
                    "width": "2px",
                    "height": "100%",
                    "-webkit-transition": "transform 0.4s linear",
                    "transition": "transform 0.4s linear"
                  },
                  "&:after": {
                    "top": "~\"calc(50% - 1px)\"",
                    "width": "100%",
                    "height": "2px"
                  }
                },
                "&[selected] .icon:before": {
                  "-webkit-transform": "scale(1, 0)",
                  "transform": "scale(1, 0)"
                }
              },
              "&:not(:first-child)": {
                "border-top": "1px solid @borderColor"
              },
