          "_customHTML": "<section class=\"pricing-table1\" group=\"Pricing Table\" plugins=\"amp-main\">\n\t<!--  -->\n\t<mbr-parameters>\n\t\t<!--  -->\n\t\t<header>Size:</header>\n\t\t<input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n\t\t<!--  -->\n\t\t<header>Title:</header>\n\t\t<input type=\"checkbox\" title=\"Title:\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Subtitle:\" name=\"showSubtitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Border:\" name=\"showBorderTitle\" condition=\"showTitle || showSubtitle\" checked>\n\t\t<input type=\"color\" title=\"Color border:\" name=\"colorBorderTitle\" condition=\"showBorderTitle\" value=\"#fc0a0a\">\n\t\t<select title=\"Align:\" name=\"alignTitle\" condition=\"showTitle || showSubtitle\">\n\t\t\t<option value=\"1\">Left</option>\n\t\t\t<option value=\"2\" selected>Center</option>\n\t\t\t<option value=\"3\">Right</option>\n\t\t</select>\n\t\t<!--  -->\n\t\t<header>Cards:</header>\n\t\t<input type=\"range\" inline title=\"Cards:\" name=\"amountCard\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n\t\t<input type=\"range\" inline title=\"Items-1:\" name=\"card1Items\" min=\"1\" max=\"6\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Items-2:\" name=\"card2Items\" min=\"1\" max=\"6\" step=\"1\" value=\"5\">\n\t\t<input type=\"range\" inline title=\"Items-3:\" name=\"card3Items\" min=\"1\" max=\"6\" step=\"1\" value=\"4\">\n\t\t<!--  -->\n\t\t<input type=\"color\" title=\"Card-1 BG:\" name=\"card1Bg\" value=\"#ffffff\">\n\t\t<input type=\"color\" title=\"Card-2 BG:\" name=\"card2Bg\" value=\"#fc0a0a\">\n\t\t<input type=\"color\" title=\"Card-3 BG:\" name=\"card3Bg\" value=\"#ffffff\">\n\t\t<!--  -->\n\t\t<input type=\"color\" title=\"Line-1\" name=\"colorLine1\" value=\"#fc0a0a\">\n\t\t<input type=\"color\" title=\"Line-2\" name=\"colorLine2\" value=\"#ffffff\">\n\t\t<input type=\"color\" title=\"Line-3\" name=\"colorLine3\" value=\"#fc0a0a\">\n\t\t<!--  -->\n\t\t<header>Background:</header>\n\t\t<fieldset type=\"background\" name=\"bg\">\n\t\t\t<input type=\"image\" title=\"Background Image:\" value=\"@PROJECT_PATH@/assets/images/background4.jpg\">\n\t\t\t<input type=\"color\" title=\"Background Color:\" value=\"#f1f1f7\" selected>\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked condition=\"bg.type!='color'\">\n\t\t<input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' && overlay\">\n\t\t<input type=\"range\" inline title=\"Overlay Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"bg.type!='color' && overlay\">\n\t</mbr-parameters>\n\t<!--  -->\n\t<!-- Overlay -->\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n\t<!-- Container -->\n\t<div class=\"container\" id=\"servicos\">\n\t\t<!-- Title -->\n\t\t<div class=\"title-block\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n            <h3 class=\"mbr-section-title mbr-fonts-style mbr-pb-2 mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">Preços Especiais</h3>\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-pb-1 mbr-light\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\">A Web 2 Easy tem preços especiais para você que pretende adquirir um site com um baixo orçamento.</h4>\n            <div class=\"line\" mbr-if=\"showBorderTitle && (showTitle || showSubtitle)\">\n                <div class=\"line-line-gray\">\n                    <div class=\"line-line-color\"></div>\n                </div>\n            </div>\n        </div>\n\t\t<!-- Content -->\n\t\t<div class=\"mbr-row mbr-pt-4 mbr-jc-c flex-al-c\">\n\t\t\t<!-- 1-card -->\n\t\t\t<div class=\"mbr-col-sm-12 mbr-col-md-7 mbr-col-lg-4 mbr-column wrapper-card\">\n\t\t\t\t<div class=\"card card-1\" mbr-class=\"{'card-br-left' : amountCard == 3}\">\n\t\t\t\t\t<!-- Title card -->\n\t\t\t\t\t<h6 mbr-text class=\"card-title align-center mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".card-1 .card-title\">SITE (max: 10 páginas)<span class=\"line-color\"></span>\n\t\t\t\t\t</h6>\n\t\t\t\t\t<!-- Cost -->\n\t\t\t\t\t<h5 class=\"card-cost mbr-fonts-style align-center mbr-pt-5\" mbr-theme-style=\"display-2\" data-app-selector=\".card-1 .card-cost\">R$699</h5>\n\t\t\t\t\t<!-- List -->\n\t\t\t\t\t<div class=\"card-list-services align-center mbr-pt-4 mbr-px-5\" mbr-list mbr-list-grow=\"card1Items\">\n\t\t\t\t\t\t<p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-1 .list-item\">20 project</p>\n\t\t\t\t\t<p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-1 .list-item\">20 project</p><p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-1 .list-item\">20 project</p><p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-1 .list-item\">20 project</p></div>\n\t\t\t\t\t<!-- Button -->\n\t\t\t\t\t<div mbr-buttons class=\"mbr-section-btn mbr-pt-5 btn-custom\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\"><a class=\"btn btn-black\" href=\"http://mobirise.com\" data-app-placeholder=\"Type Text\">GET STARTED</a></div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<!-- 2-card -->\n\t\t\t<div class=\"mbr-col-sm-12 mbr-col-md-7 mbr-col-lg-4 mbr-column wrapper-card\" mbr-if=\"amountCard > 2\">\n\t\t\t\t<div class=\"card card-2\">\n\t\t\t\t\t<!-- Title card -->\n\t\t\t\t\t<h6 mbr-text class=\"card-title align-center mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".card-2 .card-title\">SITE INSTITUCIONAL (APENAS HOME)<span class=\"line-color\"></span>\n\t\t\t\t\t</h6>\n\t\t\t\t\t<!-- Cost -->\n\t\t\t\t\t<h5 class=\"card-cost mbr-fonts-style align-center mbr-pt-5\" mbr-theme-style=\"display-1\" data-app-selector=\".card-2 .card-cost\">R$399</h5>\n\t\t\t\t\t<!-- List -->\n\t\t\t\t\t<div class=\"card-list-services align-center mbr-pt-4 mbr-px-5\" mbr-list mbr-list-grow=\"card2Items\">\n\t\t\t\t\t\t<p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-2 .list-item\">Template 1</p>\n\t\t\t\t\t<p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-2 .list-item\">Template 2</p><p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-2 .list-item\">Tempalte 3</p><p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-2 .list-item\">Template 4</p><p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-2 .list-item\">Template 5</p></div>\n\t\t\t\t\t<!-- Button -->\n\t\t\t\t\t<div mbr-buttons class=\"mbr-section-btn mbr-pt-5 btn-custom\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n\t\t\t\t\t\t<a class=\"btn-white btn\" href=\"http://mobirise.com\">GET STARTED</a>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<!-- 3-card -->\n\t\t\t<div class=\"mbr-col-sm-12 mbr-col-md-7 mbr-col-lg-4 mbr-column wrapper-card\" mbr-if=\"amountCard > 1\">\n\t\t\t\t<div class=\"card card-3\" mbr-class=\"{'card-br-right' : amountCard == 3}\">\n\t\t\t\t\t<!-- Title card -->\n\t\t\t\t\t<h6 mbr-text class=\"card-title align-center mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".card-3 .card-title\">SITE AMP<br><span class=\"line-color\"></span>\n\t\t\t\t\t</h6>\n\t\t\t\t\t<!-- Cost -->\n\t\t\t\t\t<h5 class=\"card-cost mbr-fonts-style align-center mbr-pt-5\" mbr-theme-style=\"display-2\" data-app-selector=\".card-3 .card-cost\">R$999</h5>\n\t\t\t\t\t<!-- List -->\n\t\t\t\t\t<div class=\"card-list-services align-center mbr-pt-4 mbr-px-5\" mbr-list mbr-list-grow=\"card3Items\">\n\t\t\t\t\t\t<p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-3 .list-item\">200 project</p>\n\t\t\t\t\t<p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-3 .list-item\">200 project</p><p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-3 .list-item\">200 project</p><p class=\"list-item mbr-fonts-style mbr-text mbr-light mbr-py-1\" mbr-theme-style=\"display-4\" data-app-selector=\".card-3 .list-item\">200 project</p></div>\n\t\t\t\t\t<!-- Button -->\n\t\t\t\t\t<div mbr-buttons class=\"mbr-section-btn mbr-pt-5 btn-custom\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\"><a class=\"btn btn-black\" href=\"http://mobirise.com\" data-app-placeholder=\"Type Text\">GET STARTED</a></div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</section>",
